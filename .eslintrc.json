{
  "extends": "airbnb-base",
  "plugins": [
    "import"
  ],
  "rules": {
    "indent": [2, 4, {"SwitchCase": 1}],
    "comma-spacing": [2, {"before": false, "after": true}],
    "no-console":0,
    "object-curly-spacing": 0,
    "consistent-return": 0,
    "strict": 0,
    "func-names": 0,
    "no-param-reassign": 0,
    "no-undef": 0,
    "no-use-before-define": 0,
    "no-unused-vars": 0
  },
  "env": {
    "browser": true,
    "node": true
  }
}